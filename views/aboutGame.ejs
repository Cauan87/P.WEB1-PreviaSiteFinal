<title><%= title %></title>
    <%- include('./partials/header') %> 
    <link rel="stylesheet" href="/stylesheets/style.css">
<body class="bg-custom">
    <div class="container mt-5">
        <div class="row" >
            <div class="col-md-6">
                <h1><%= title %></h1>
                <p class="descJogo mt-5"><%= desc %></p>
            </div>
            <div class="col-md-6">
                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="https://giffiles.alphacoders.com/217/217181.gif" alt="Slide 1">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="https://giffiles.alphacoders.com/217/217181.gif" alt="Slide 2">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="https://giffiles.alphacoders.com/217/217181.gif" alt="Slide 3">
                        </div>
                    </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                </div>
            </div>
        </div>
    </div>
    <% if (!title) { %>
        <p>Não há informações disponíveis sobre <%= title %>.</p>
    <% } %>
<!-- <> -->



